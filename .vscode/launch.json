    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch API Server via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "api"
            ],
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch static server via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "static"
            ],
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
       
    ]
}